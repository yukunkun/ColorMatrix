        <item name="android:layout_width">@dimen/activity_main_submit_width</item>
        <item name="android:layout_height">@dimen/activity_main_title_hight</item>
        <item name="android:background">@drawable/main_start_button</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
