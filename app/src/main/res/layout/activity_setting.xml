<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.matrix.yukun.matrix.setting_module.SettingActivity">
    <RelativeLayout
        android:background="@color/color_252525"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimens_60">
        <ImageView
            android:padding="15dp"
            android:onClick="SetingBack"
            android:src="@mipmap/abc_ic_ab_back_mtrl_am_alpha"
            android:layout_width="@dimen/dimens_60"
            android:layout_height="match_parent" />
        <com.mcxtzhang.pathanimlib.StoreHouseAnimView
            android:id="@+id/storeHouseAnimView"
            android:layout_width="90dp"
            android:layout_centerInParent="true"
            android:paddingTop="20dp"
            android:paddingLeft="2dp"
            android:layout_height="match_parent" />
    </RelativeLayout>
    <ListView
        android:id="@+id/mListview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layoutAnimation="@anim/layout_anim"
        />
</LinearLayout>
